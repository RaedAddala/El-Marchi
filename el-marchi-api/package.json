{
  "name": "@el-marchi-api/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "check:cd": "pnpx madge --circular apps/el-marchi-api/src",
    "start": "nx serve el-marchi-api --configuration=development",
    "start:prod": "nx serve el-marchi-api --configuration=production",
    "build": "nx build el-marchi-api --configuration=development",
    "build:prod": "nx build el-marchi-api --configuration=production",
    "test": "nx test el-marchi-api",
    "test:watch": "nx test el-marchi-api --watch",
    "test:coverage": "nx test el-marchi-api --coverage",
    "test:debug": "nx test el-marchi-api --debug",
    "e2e": "nx e2e el-marchi-api-e2e",
    "e2e:watch": "nx e2e el-marchi-api-e2e --watch",
    "lint": "nx lint el-marchi-api",
    "lint:fix": "nx lint el-marchi-api --fix",
    "format:check": "prettier --check \"**/*.{js,ts}\"",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "affected:build": "nx affected --target=build",
    "affected:e2e": "nx affected --target=e2e",
    "graph": "nx graph",
    "clean": "rimraf dist && rimraf .nx && nx reset",
    "documentation": "pnpx @compodoc/compodoc -p apps/el-marchi-api/tsconfig.app.json -d dist/documentation --theme material"
  },
  "private": true,
