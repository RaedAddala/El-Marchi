    "@nestjs/swagger": "^11.0.3",
    "@nestjs/typeorm": "^11.0.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "axios": "^1.7.9",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "helmet": "^8.0.0",
    "openapi3-ts": "^4.4.0",
