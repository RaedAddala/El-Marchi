{
  "name": "@el-marchi-api/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nx serve",
    "start:prod": "nx serve --configuration=production",
    "build": "nx build",
    "build:prod": "nx build --configuration=production",
    "test": "nx test",
    "test:watch": "nx test --watch",
    "test:coverage": "nx test --configuration=ci",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "e2e": "pnpm build & nx e2e e2e",
    "e2e:watch": "nx e2e e2e --watch",
    "lint": "nx lint",
    "lint:fix": "nx lint --fix",
    "format": "prettier --write \"{src,e2e,apps}/**/*.ts\"",
    "format:check": "prettier --check \"{src,e2e,apps}/**/*.ts\"",
    "docs:dev": "nx compodoc --configuration=dev",
    "docs:prod": "nx compodoc --configuration=production",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "affected:build": "nx affected --target=build",
    "affected:e2e": "nx affected --target=e2e",
    "graph": "nx graph",
    "clean": "rimraf dist && nx reset",
    "clean:coverage": "rimraf dist && rimraf coverage && nx reset"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "axios": "^1.7.9",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/js": "^9.17.0",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@nx/eslint": "20.3.0",
    "@nx/eslint-plugin": "20.3.0",
    "@nx/jest": "20.3.0",
    "@nx/js": "20.3.0",
    "@nx/nest": "20.3.0",
    "@nx/node": "20.3.0",
    "@nx/web": "20.3.0",
    "@nx/webpack": "20.3.0",
    "@nx/workspace": "20.3.0",
    "@swc-node/register": "~1.10.9",
    "@swc/core": "~1.10.4",
    "@swc/helpers": "~0.5.15",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "~22.10.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nx": "20.3.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
